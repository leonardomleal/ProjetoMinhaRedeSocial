SELECT 
	Usuarios.Nome,
	Postagens.Texto,
	Postagens.Data,
	Postagens.Curtidas,
	(SELECT COUNT(Comentarios.Id) FROM Comentarios WHERE Comentarios.PostagemId = Postagens.Id) AS Comentarios
FROM 
	Postagens
INNER JOIN
	Usuarios ON Postagens.UsuarioId = Usuarios.Id






